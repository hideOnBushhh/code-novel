{
  "name": "code-novel2",
  "displayName": "Code Novel2",
  "description": "代码注释小说2",
  "version": "0.1.0",
  "publisher": "mathon",
  "icon": "fish.png",
  "engines": {
    "vscode": "^1.34.0"
  },
  "keywords": [
    "摸鱼",
    "看书",
    "神器",
    "代码看书"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.openNovel",
        "key": "ctrl+alt+,",
        "mac": "cmd+alt+,"
      },{
        "command": "extension.nextpage",
        "key": "ctrl+alt+j",
        "mac": "cmd+alt+j"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "eslint": "^5.13.0",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iyim/code-novel"
  }
}
